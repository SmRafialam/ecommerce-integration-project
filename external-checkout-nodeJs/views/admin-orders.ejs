<h1>Admin Orders</h1>
<ul>
  <% orders.forEach(order => { %>
    <li>
      Order ID: <%= order.id %><br>
      Email: <%= order.billing.email %><br>
      Status: <%= order.status %><br>
      Product(s): 
      <ul>
        <% order.line_items.forEach(item => { %>
          <li><%= item.name %> x <%= item.quantity %></li>
        <% }) %>
      </ul>
    </li>
    <hr>
  <% }) %>
</ul>
